<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/padang_pasir"
    tools:context=".UI.FavoritePage">

    <TextView
        android:id="@+id/textView27"
        android:layout_width="match_parent"
        android:layout_height="80dp"
        android:background="@drawable/shape_biru"
        android:fontFamily="@font/montserrat_bold"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:paddingTop="23dp"
        android:textSize="24dp"
        android:text="Settings"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.cardview.widget.CardView
        android:id="@+id/cv1"
        android:layout_width="261dp"
        android:layout_height="76dp"
        android:layout_marginTop="100dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:backgroundTint="@color/white"
        app:layout_constraintTop_toBottomOf="@+id/textView27">

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="86dp"
            android:layout_height="match_parent"
            android:padding="15dp"
            app:srcCompat="@drawable/big_icon_destination" />

        <TextView
            android:id="@+id/textView10"
            android:layout_width="172dp"
            android:layout_height="wrap_content"
            android:fontFamily="@font/montserrat_bold"
            android:paddingStart="100dp"
            android:paddingTop="30dp"
            android:text="Profile"
            android:textColor="@color/black"
            android:textSize="12dp" />
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/cv2"
        android:layout_width="261dp"
        android:layout_height="76dp"
        android:layout_marginTop="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:backgroundTint="@color/white"
        app:layout_constraintTop_toBottomOf="@+id/cv1">

        <ImageView
            android:id="@+id/imageView1"
            android:layout_width="86dp"
            android:layout_height="match_parent"
            android:padding="15dp"
            app:srcCompat="@drawable/big_icon_destination" />

        <TextView
            android:id="@+id/textView11"
            android:layout_width="172dp"
            android:layout_height="wrap_content"
            android:fontFamily="@font/montserrat_bold"
            android:paddingStart="100dp"
            android:paddingTop="30dp"
            android:text="Contact"
            android:textColor="@color/black"
            android:textSize="12dp" />
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/cv4"
        android:layout_width="261dp"
        android:layout_height="76dp"
        android:layout_marginTop="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:backgroundTint="@color/white"
        app:layout_constraintTop_toBottomOf="@+id/cv3">

        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="86dp"
            android:layout_height="match_parent"
            android:padding="15dp"
            app:srcCompat="@drawable/big_icon_destination" />

        <TextView
            android:id="@+id/textView14"
            android:layout_width="172dp"
            android:layout_height="wrap_content"
            android:fontFamily="@font/montserrat_bold"
            android:paddingStart="100dp"
            android:paddingTop="30dp"
            android:text="Log Out"
            android:textColor="@color/black"
            android:textSize="12dp" />
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/cv3"
        android:layout_width="261dp"
        android:layout_height="76dp"
        android:layout_marginTop="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:backgroundTint="@color/white"
        app:layout_constraintTop_toBottomOf="@+id/cv2">

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="86dp"
            android:layout_height="match_parent"
            android:padding="15dp"
            app:srcCompat="@drawable/big_icon_destination" />

        <TextView
            android:id="@+id/tvTitleDestinasi"
            android:layout_width="172dp"
            android:layout_height="wrap_content"
            android:fontFamily="@font/montserrat_bold"
            android:paddingStart="100dp"
            android:paddingTop="30dp"
            android:text="Version"
            android:textColor="@color/black"
            android:textSize="12dp" />
    </androidx.cardview.widget.CardView>

    <ImageView
        android:id="@+id/ivBack10"
        android:layout_width="53dp"
        android:layout_height="52dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="24dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView27"
        app:srcCompat="@drawable/icon_back_button" />

</androidx.constraintlayout.widget.ConstraintLayout>